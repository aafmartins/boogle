<div class="headerImageContainer">

  <div>
    <img class="headerImage" src="/images/reading.png" alt="" />
  </div>

  <h1>My Saved Books List</h1>

  <div class="buttonContainer">
    <a href="/bookshelf/my-saved-books">
      <button disabled class="navigatingBtn" id="mySavedBooksBtnInSavedBook">
        My Saved Books
      </button>
    </a>
    <a href="/bookshelf/my-created-books" id="myCreatedBooksBtnInSavedBook">
      <button class="navigatingBtn">
        My Created Books
      </button>
    </a>
  </div>
</div>

{{#unless result}}
<div class="nothing-here-container">
<h3>You have no saved books yet...</h3>
</div>
{{/unless}}

    <a href="/">
        <button class="create-btn">Save a book!</button>
    </a>

<ul>
  {{#each result}}
    <li>
      <div class="mainContainer">

        <div class="detailsContainer">
          <h2>
            {{this.title}}
          </h2>

          <h4>
            {{this.authors}}
          </h4>
          <a class="viewMoreBtn-container" href="/my-saved-books/{{this._id}}">
            <button  class="viewMoreBtn">
              Read More
            </button>
          </a>
        </div>

        <div class="imageContainer">
          <img src="{{this.bookPictureUrl}}" alt="" />
        </div>

      </div>
    </li>
  {{/each}}
</ul>